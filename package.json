{
  "name": "sw-assistant",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "build":
      "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev":
      "webpack-dev-server --config=configs/webpack/webpack.config.js ",
    "start-prod": "npm run build && node express.js",
    "profile":
      "webpack -p --config=configs/webpack/webpack.config.js--profile --json > stats.json",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "happypack": "^5.0.0",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.4",
    "react-addons-test-utils": "^15.6.2",
    "sass-loader": "^7.0.1",
    "source-map": "0.7.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.3",
    "webpack-jarvis": "^0.3.2",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-loadable": "^5.3.4",
    "@types/react-router-dom": "^4.2.6",
    "jsdom-quokka-plugin": "^1.0.13",
    "mobx": "^4.2.1",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^5.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "react-loadable": "^5.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-virtualized": "^9.18.5",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  },
  "postcss": {}
}
